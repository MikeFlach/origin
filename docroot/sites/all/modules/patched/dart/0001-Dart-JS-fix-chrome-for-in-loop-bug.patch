From f574fff9464be622385981716bbc1032550639cc Mon Sep 17 00:00:00 2001
From: Henry Wan <hwan@maxim.com>
Date: Fri, 17 Feb 2012 12:55:33 -0500
Subject: [PATCH] Dart JS - fix chrome for-in loop bug

---
 docroot/sites/all/modules/contrib/dart/dart.js |    8 ++++----
 1 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/docroot/sites/all/modules/contrib/dart/dart.js b/docroot/sites/all/modules/contrib/dart/dart.js
index a66a95b..f3b28c7 100644
--- a/docroot/sites/all/modules/contrib/dart/dart.js
+++ b/docroot/sites/all/modules/contrib/dart/dart.js
@@ -67,10 +67,10 @@ Drupal.DART.keyVals = function(vals) {
   var ad = '';
   for(var key in vals) {
     value = vals[key];
-    for(var val in value) {
-      v = value[val];
-      ad += this.keyVal(key, v['val'], v['eval']);
-    }
+    for (var i = 0, max = value.length; i < max; i++) {
+     v = value[i];
+     ad += this.keyVal(key, v['val'], v['eval']);
+    } 
   }
   return ad;
 };
-- 
